<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox id="TesterVBox" prefHeight="400.0" prefWidth="600.0" styleClass="background" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="test.TesterController">
    <stylesheets>
        <URL value="@/styles/Styles.css"/>
    </stylesheets>
    <children>
        <Label text="Test the Following" textAlignment="CENTER" underline="true">
           <font>
              <Font name="AGA Arabesque" size="36.0" />
           </font>
        </Label>
         <Button onAction="#loadAlertScene" text="Alert" textAlignment="CENTER" />
         <Button onAction="#loadAppointmentScene" text="Appointment" textAlignment="CENTER" />
         <Button onAction="#loadAppointmentEditScene" text="Appointment Edit" textAlignment="CENTER" />
         <Button onAction="#loadCustomerScene" text="Customer" textAlignment="CENTER" />
         <Button onAction="#loadCustomerEditScene" text="Customer Edit" textAlignment="CENTER" />
         <Button onAction="#loadLoginScene" text="Login" textAlignment="CENTER" />
         <Button onAction="#loadUserEditScene" text="User Edit" textAlignment="CENTER" />
         <Button onAction="#loadMainScene" text="Main Menu" textAlignment="CENTER" />
    </children>
</VBox>
